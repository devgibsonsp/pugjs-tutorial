doctype html
html
    head
    body
        h1 For/Each loop
        - const names = ["Dom", "Sophie", "Mark"]
        for n, i in names
            p=n + " - " + i
        - const grades = {"Web Dev": 85, "Software Design": 76}
        each n, i in grades
            p=n + " - " + i
        else
            strong No Values Here!

        each n, i in []
            p=n + " - " + i
        else
            strong No Values Here!